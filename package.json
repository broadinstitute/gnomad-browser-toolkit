{
  "name": "react-gnomad",
  "version": "0.0.1",
  "description": "A component library for variant browsers",
  "main": "dist/es/index.js",
  "repository": "https://github.com/macarthur-lab/react-gnomad",
  "author": "Matthew Solomonson <msolomon@broadinstitute.org>",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:es": "cross-env NODE_ENV=production cross-env BABEL_ENV=es babel src --out-dir dist/es --ignore demo,*.example.js,*.test.js",
    "build:umd": "cross-env NODE_ENV=production webpack --config webpack.config.umd.js",
    "prebuild": "rimraf dist",
    "demo": "cross-env NODE_ENV=development webpack-dev-server --hot --config webpack.config.js"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.4.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-webpack-alias": "2.1.2",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.2.1",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-2": "6.22.0",
    "config": "1.25.1",
    "cross-env": "3.2.3",
    "css-loader": "0.27.3",
    "eslint": "3.17.1",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "graphql-fetch": "1.0.1",
    "npm-run-all": "4.0.2",
    "postcss-cssnext": "2.9.0",
    "postcss-loader": "1.3.3",
    "rimraf": "2.6.1",
    "style-loader": "0.13.2",
    "stylelint": "7.9.0",
    "stylelint-config-standard": "16.0.0",
    "watch": "1.0.2",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.2"
  },
  "peerDependencies": {
    "react": "15.4.2",
    "react-dom": "15.4.2"
  },
  "dependencies": {
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-hot-loader": "^3.0.0-beta.6"
  }
}
