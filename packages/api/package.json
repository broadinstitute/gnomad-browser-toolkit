{
  "name": "gnomad-gql",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/babel-node --inspect node_modules/.bin/tape './src/tests/elastic-spec.js' | ./node_modules/.bin/tspec",
    "test:watch": "./node_modules/.bin/watch 'npm test' src -d",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-server",
    "start": "GRAPHQL_PORT=8000 MONGO_URL=mongodb://localhost:27017/exac node --max_old_space_size=12000 build/server.js",
    "dev": "nodemon --exec GRAPHQL_PORT=8007 GNOMAD_MONGO_URL=mongodb://localhost:27017/exac EXACV1_MONGO_URL=mongodb://localhost:27017/exac babel-node src/server.js --max_old_space_size=4096",
    "vm": "nodemon --exec GRAPHQL_PORT=8000 MONGO_URL=mongodb://localhost:27017/exac babel-node src/server.js --max_old_space_size=4096",
    "prod": "GRAPHQL_PORT=8000 MONGO_URL=mongodb://localhost:27017/exac node --max_old_space_size=65536  build/server.js",
    "docker": "node build/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.1",
    "elasticsearch": "^13.3.1",
    "express": "^4.14.0",
    "express-graphql": "^0.5.4",
    "graphql": "^0.7.2",
    "graphql-tester": "0.0.5",
    "isomorphic-fetch": "^2.2.1",
    "mongodb": "^2.2.11",
    "nodemon": "^1.11.0",
    "ramda": "^0.22.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-node": "^6.5.3",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "tape-promise": "^2.0.1"
  }
}
