{
  "name": "@lens/demo",
  "version": "0.0.1",
  "description": "A React component library for building variant browsers",
  "main": "dist/es/index.js",
  "repository": "https://github.com/macarthur-lab/react-gnomad",
  "author": "Matthew Solomonson <msolomon@broadinstitute.org>",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:es": "cross-env NODE_ENV=production cross-env BABEL_ENV=es babel src --out-dir dist/es --copy-files --ignore demo,*.example.js,*.test.js,*get-test-data.js,*spec.js,",
    "build:umd": "cross-env NODE_ENV=production webpack --config webpack.config.umd.js",
    "prebuild": "rimraf dist",
    "demo": "cross-env NODE_ENV=development webpack-dev-server --hot --config webpack.config.js",
    "demo:cold": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "test:data": "babel-node src/Tracks/GenericTableTrack/get-test-data.js",
    "test": "mocha",
    "test:watch": "mocha --watch"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "config": "^1.25.1",
    "cross-env": "^3.2.3",
    "css-loader": "^0.27.3",
    "dsv-loader": "^2.0.0",
    "npm-run-all": "^4.0.2",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.3.3",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.2",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "svg-inline-loader": "^0.7.1",
    "watch": "^1.0.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2",
    "@lens/test": "1.0.0"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6"
  }
}
