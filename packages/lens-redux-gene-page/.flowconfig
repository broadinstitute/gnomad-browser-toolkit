[ignore]
.*/dist/*.
#.*/src/**/spec.js
*/node_modules/*

[include]
.*/packages/*/src/*.

[libs]
.*/src/*.
flow-typed/
./node_modules/fbjs/flow/lib

[options]
suppress_type=$FlowIssue
module.ignore_non_literal_requires=true
module.name_mapper='.*\(.css\)' -> 'CSSModule'
module.system=haste
strip_root=true
module.name_mapper='.*\(services\)' -> '<PROJECT_ROOT>/src/services'
