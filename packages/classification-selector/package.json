    "@rollup/plugin-node-resolve": "8.1.0",
    "@storybook/react": "6.0.0-beta.31",
    "@types/lodash": "4.14.155",
    "@types/react": "16.9.38",
    "@types/react-dom": "16.9.8",
    "core-js": "3.6.5",
    "csstype": "2.6.11",
    "cypress": "4.9.0",
    "rollup": "2.18.0",
    "typescript": "3.9.5"
  },
  "dependencies": {
    "@material-ui/core": "4.10.2",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
