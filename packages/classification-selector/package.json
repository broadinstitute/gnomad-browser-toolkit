{
  "name": "@gnomad/classification-selector",
  "version": "0.5.0",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "files": ["lib"],
  "scripts": {
    "jest": "jest --coverage",
    "jest:watch": "jest --watch",
    "storybook": "start-storybook --port 3200",
    "cypress:open": "cypress open",
    "clean": "rm -rf lib",
    "typecheck": "tsc --noEmit",
