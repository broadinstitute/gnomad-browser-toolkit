{
  "name": "@gnomad/classification-selector",
  "version": "0.7.1",
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "jest": "jest --coverage",
    "jest:watch": "jest --watch",
    "test": "jest --coverage",
    "storybook": "start-storybook --port 3200",
    "cypress:open": "cypress open",
    "clean": "rm -rf lib",
    "typecheck": "tsc --noEmit",
    "tsc:declaration": "tsc --declaration --emitDeclarationOnly --outDir lib",
    "build": "pnpm clean && pnpm typecheck && pnpm tsc:declaration && pnpm rollup && cp src/cypressTestDataAttrs.json lib/",
    "rollup": "IS_ROLLUP=true rollup -c"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "5.0.4",
    "@rollup/plugin-commonjs": "13.0.0",
    "@rollup/plugin-node-resolve": "8.1.0",
    "@storybook/react": "6.0.0-beta.31",
    "@types/lodash": "4.14.155",
    "core-js": "3.6.5",
    "csstype": "2.6.11",
    "cypress": "4.9.0",
    "rollup": "2.18.0",
    "typescript": "3.9.5"
  },
  "dependencies": {
    "@material-ui/core": "4.10.2",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "fontsource-roboto": "2.2.6",
    "lodash": "4.17.21"
  }
