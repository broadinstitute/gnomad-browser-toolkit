    "rollup": "IS_ROLLUP=true rollup -c"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "5.0.4",
