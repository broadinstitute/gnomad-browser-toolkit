    "@storybook/react-webpack5": "7.4.4",
    "core-js": "3.6.5",
    "cypress": "4.9.0",
    "rollup": "^3.0.0",
    "storybook": "7.4.4",
    "csstype": "*",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
